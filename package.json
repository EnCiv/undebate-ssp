{
    "name": "undebate-ssp",
    "version": "v0.0.0",
    "description": "Undebate Self Service Portal",
    "main": "index.js",
    "scripts": {
        "db-up": "start /b mongod --quiet --dbpath ./tmp/db --port 27017",
        "db-down": "mongo --eval \"db.getSiblingDB('admin').shutdownServer(); quit()\"",
        "dev": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"sleep 20 && npm run hot-server\" \"sleep 15 && npm run hot-client\"",
        "hot-client": "webpack-dev-server --config webpack-dev.config.js --devtool source-map --host 0.0.0.0",
        "hot-server": "nodemon --inspect dist/start.js",
        "hot-transpile": "babel --watch app --ignore **/__tests__ --out-dir dist --source-maps",
        "packbuild": "webpack --config webpack-prod.config.js",
        "postinstall": "bash postinstall.sh",
        "prestart": "npm run packbuild",
        "start": "node --max_old_space_size=460 dist/start.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "transpile": "babel app --ignore **/__tests__ --out-dir dist --source-maps",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "svgr": "svgr --icon --filename-case kebab assets/svg --out-dir app/svgr && rm app/svgr/index.js",
        "_lint": "eslint --config .eslintrc.js --ext js --ext jsx",
        "lint": "npm run _lint -- --fix ./app ./stories",
        "format": "prettier --write './**/*.{js,jsx,css,scss,md,json}' --config ./.prettierrc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EnCiv/undebate-ssp.git"
    },
    "keywords": [
        "node",
        "express",
        "mongo",
        "socketio",
        "user",
        "login",
        "logout",
        "forgotpassword",
        "join"
    ],
    "author": "David Fridley",
    "license": "SEE LICENSE IN license.txt",
    "bugs": {
        "url": "https://github.com/EnCiv/undebate-ssp/issues"
    },
    "homepage": "https://github.com/EnCiv/undebate-ssp#readme",
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@storybook/addon-actions": "^6.1.18",
        "@storybook/addon-essentials": "^6.1.18",
        "@storybook/addon-links": "^6.1.18",
        "@storybook/react": "^6.1.18",
        "@svgr/cli": "^5.5.0",
        "concurrently": "^5.3.0",
        "eslint": "^8.4.1",
        "eslint-config-airbnb": "^19.0.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "nodemon": "^2.0.7",
        "prettier": "^2.5.1",
        "react-calendar": "^3.4.0"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-transform-react-inline-elements": "^7.12.1",
        "@babel/plugin-transform-regenerator": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/runtime": "^7.12.5",
        "babel-loader": "^8.2.2",
        "civil-client": "github:EnCiv/civil-client",
        "civil-server": "github:EnCiv/civil-server#main",
        "classnames": "^2.2.6",
        "compression-webpack-plugin": "^7.1.2",
        "humanize-string": "^2.1.0",
        "isemail": "^3.2.0",
        "isomorphic-mongo-objectid": "^1.0.9",
        "moment": "^2.29.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hot-loader": "^4.12.10",
        "react-jss": "^10.1.1",
        "react-textarea-autosize": "^8.3.3",
        "undebate": "github:EnCiv/undebate#civ-serv",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.2"
    },
    "engines": {
        "node": "~14.x.x",
        "npm": "~7.17.x"
    }
}
